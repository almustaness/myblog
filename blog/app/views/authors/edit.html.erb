<h1 class="page-title">Edit your account</h1>
<hr>

<div>
  <%= form_for @auth do |a| %>
  <%= a.label :UserName %>
  <%= a.text_field :authName %>
  <%= a.label :email %>
  <%= a.email_field :email %>
  <%= a.label :password %>
  <%= a.password_field :password %>
  <%= a.submit "Update" %>
  <% end %>
</div>